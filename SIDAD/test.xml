<?xml version="1.0" encoding="UTF-8"?>
<系统 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="接口协议模型.xsd">
	<名称>xxxt</名称>
	<类型>xxxt</类型>
	<节点>
		<成员>
			<编号>0x01</编号>
			<名称>JJDD子系统</名称>
			<组播地址>192.168.0.255</组播地址>
			<交换机>
				<编号>0x04</编号>
				<名称>JJDD交换机1</名称>
				<端口>1</端口>
				<端口>2</端口>
			</交换机>
			<设备>
				<编号>0x02</编号>
				<名称>JJDD1号设备</名称>
				<类型>计算设备</类型>
				<网络接口>
					<MAC地址>0123456789ab</MAC地址>
					<IP>192.168.0.1</IP>
					<掩码>255.255.255.0</掩码>
					<网关>192.168.0.254</网关>
				</网络接口>
			</设备>
			<设备>
				<编号>0x03</编号>
				<名称>JJDD2号设备</名称>
				<类型>计算设备</类型>
				<网络接口>
					<MAC地址>1234567890cd</MAC地址>
					<IP>192.168.0.2</IP>
					<掩码>255.255.255.0</掩码>
					<网关>192.168.0.254</网关>
				</网络接口>
			</设备>
		</成员>
	</节点>
	<软件集合>
		<软件>
			<软件标识>0xab</软件标识>
			<名称>JJDD软件</名称>
			<类型>计算软件</类型>
			<域标识>1</域标识>
			<端口范围>0123-2345</端口范围>
		</软件>
		<软件>
			<软件标识>0xbb</软件标识>
			<名称>JJDD接收软件</名称>
			<类型>显控软件</类型>
			<域标识>1</域标识>
			<端口范围>2345-7823</端口范围>
		</软件>
	</软件集合>
	<消息集合>
		<消息>
			<名称>攻击目标</名称>
			<标识>TAttackTarget</标识>
			<消息类型>DDS</消息类型>
			<传输类型>一次性传播</传输类型>
			<结构>
				<成员 类型="unsignedShort" 字段标识="TargetNo"/>
				<成员 类型="unsignedShort" 字段标识="AssignedmissleNum"/>
				<成员 类型="float" 字段标识="PermietedPLAngelA"/>
			</结构>
		</消息>
		<消息>
			<名称>JJDD攻击命令</名称>
			<标识>TAttackCmdToJJDD</标识>
			<消息类型>DDS</消息类型>
			<传输类型>一次性传播</传输类型>
			<结构>
				<成员 类型="double" 字段标识="Timestamp" 名称="时戳"/>
				<成员 类型="unsignedShort" 字段标识="CommandWord"/>
				<成员 类型="unsignedLong" 字段标识="TimeOverHead"/>
				<成员 类型="nonBasic" 字段标识="AttackTargetSeq" 非基本类型名="TAttackTarget" 最大序列长度="8"/>
			</结构>
		</消息>
	</消息集合>
	<接口关系集合>
		<DDS接口关系>
			<接口关系>
				<域标识>1</域标识>
				<发布成员>
					<软件标识>0xab</软件标识>
					<发送条件>根据条件发送</发送条件>
					<数据率>1次</数据率>
				</发布成员>
				<接收成员>
					<软件标识>0xbb</软件标识>
					<接收条件>总是接收</接收条件>
				</接收成员>
				<消息>TAttackCmdToJJDD</消息>
				<配置>
					<DDS配置>
						<Qos策略 name="oneTime">
								<publisher_qos>
									<publisher_name>
										<name>JJDD发布者</name>
										<role_name>JJDDpublisher</role_name>
									</publisher_name>
								</publisher_qos>
								<participant_factory_qos>
									<entity_factory>
										<autoenable_created_entities>true</autoenable_created_entities>
									</entity_factory>
								</participant_factory_qos>
						</Qos策略>
						<说明></说明>
					</DDS配置>
				</配置>
			</接口关系>
		</DDS接口关系>
		<软件部署关系>
			<部署关系>
				<设备编号>0x02</设备编号>
				<软件标识>0xab</软件标识>
			</部署关系>
			<部署关系>
				<设备编号>0x03</设备编号>
				<软件标识>0xbb</软件标识>
				<说明/>
			</部署关系>
		</软件部署关系>
	</接口关系集合>
</系统>
